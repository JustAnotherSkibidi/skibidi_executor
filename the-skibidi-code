local FLY_VALUE = false
local INPUT_VALUE = false

FLY.MouseButton1Click:Connect(function()
	if INVISIBLE_VALUE == true then VISIBILITY() end
	INVISIBLE_VALUE = false
	TweenColor(Options:FindFirstChild("INVISIBLE"), Color3.fromRGB(255, 150, 150))
	local CHAR = PLR.Character
	local HRM = CHAR:FindFirstChild("Humanoid").RootPart
	if FLY_VALUE == false then TweenColor(FLY, Color3.fromRGB(107, 255, 137))
		FLY_VALUE = true
		INPUT_VALUE = false
	else TweenColor(FLY, Color3.fromRGB(255, 150, 150))
		FLY_VALUE = false
		INPUT_VALUE = false
		HRM.Anchored = false
	end
end)

local RunService = game:GetService("RunService")
local UIS = game:GetService("UserInputService")
local Speed = 2.5

function FLIGHT(key)
	local CHAR = PLR.Character
	local HRM = CHAR:FindFirstChild("Humanoid").RootPart
	if CHAR:FindFirstChild("Humanoid") then
		-- FRONT SIDE BACK KEYS
		if key == "W" then HRM.CFrame = (HRM.CFrame * CFrame.new(0,0,-Speed))
		elseif key == "A" then HRM.CFrame = (HRM.CFrame * CFrame.new(-Speed,0,0))
		elseif key == "S" then HRM.CFrame = (HRM.CFrame * CFrame.new(0,0,Speed))
		elseif key == "D" then HRM.CFrame = (HRM.CFrame * CFrame.new(Speed,0,0))
			-- UP AND DOWN KEYS
		elseif key == "E" then HRM.CFrame = (HRM.CFrame * CFrame.new(0,Speed,0))
		elseif key == "Q" then HRM.CFrame = (HRM.CFrame * CFrame.new(0,-Speed,0))
		end
	end
end

function KEY_PRESS()
	-- FRONT SIDE BACK KEYS
	if UIS:IsKeyDown("W") then FLIGHT("W") end
	if UIS:IsKeyDown("A") then FLIGHT("A") end
	if UIS:IsKeyDown("S") then FLIGHT("S") end
	if UIS:IsKeyDown("D") then FLIGHT("D") end
	-- UP AND DOWN KEYS
	if UIS:IsKeyDown("E") then FLIGHT("E") end
	if UIS:IsKeyDown("Q") then FLIGHT("Q") end
end

function FACE_CAM()
	local CAMERA = workspace.CurrentCamera
	if FLY_VALUE == true and INPUT_VALUE == true then
		local CHAR = PLR.Character
		local HRM = CHAR:FindFirstChild("Humanoid").RootPart
		local CAMERA_POS = CFrame.new(workspace.CurrentCamera.CFrame.Position) -- CHAR:FindFirstChild("Humanoid").CameraOffset)
		local CAMERA_CFRAME = CAMERA_POS * CAMERA.CFrame - CAMERA.CFrame.Position
		local lookVector = CAMERA_CFRAME.LookVector

		local TARGET = CFrame.new(HRM.CFrame.Position, HRM.CFrame.Position + 
			Vector3.new(lookVector.X, lookVector.Y, lookVector.Z))

		HRM.CFrame = TARGET
	end
end
task.spawn(function()
	while true do task.wait()
		if FLY_VALUE == true and INPUT_VALUE == true then
			local CHAR = PLR.Character
			if CHAR:FindFirstChild("Humanoid") then
				KEY_PRESS()
			end
		end
	end
end)

RunService.RenderStepped:Connect(function()
	if FLY_VALUE == true and INPUT_VALUE == true then
		local CHAR = PLR.Character
		if CHAR:FindFirstChild("Humanoid") then
			FACE_CAM()
		end
	end
end)

UIS.InputBegan:Connect(function(key)
	local CHAR = PLR.Character
	local HRM = CHAR:FindFirstChild("Humanoid").RootPart
	if key.KeyCode == Enum.KeyCode.Space then
		if FLY_VALUE == true then
			if INPUT_VALUE == true then
				INPUT_VALUE = false
				HRM.Anchored = false
			else
				INPUT_VALUE = true
				HRM.Anchored = true
			end
		end
	end
end)

