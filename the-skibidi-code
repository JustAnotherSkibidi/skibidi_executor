-- TELEPORT CODE

ChosenUSER.FocusLost:Connect(function()
	local TEXT = ChosenUSER.Text
	local PLAYERS = game:GetService("Players")
	local MORE = 0
	for i,v in pairs(PLAYERS:GetChildren()) do
		if string.find(v.Name:lower(),TEXT:lower()) then
			PLR.Character:PivotTo(v.Character.Torso.CFrame * CFrame.new(0,0,-5))
			MORE += 1
		end
	end
	if MORE >= 2 then ChosenUSER.Text = "" return end
	ChosenUSER.Text = ""
end)
