-- INVISIBLE CODE

local OFFSET = 1000
local OLD_Y = nil

local CAM = workspace.CurrentCamera

function GET_FAKECHAR(CHAR)
	local MODEL = Instance.new("Model",workspace)
	MODEL.Name = "FAKE_CHAR"
	local HUM = Instance.new("Humanoid",MODEL)
	for i,v in pairs(CHAR:GetChildren()) do
		if v:IsA("BasePart") then
			local CLONE = v:Clone()
			CLONE.Parent = MODEL
			CLONE.Anchored = true
			CLONE.CanCollide = false
			if v.Name == CHAR.PrimaryPart.Name then
				MODEL.PrimaryPart = CLONE
			end
			for i,v in pairs(CLONE:GetChildren()) do v:Destroy() end
		end
	end
	return MODEL
end

local WALK_PART = nil
local FAKE_CHAR = nil
function VISIBILITY() FLY_VALUE = false INPUT_VALUE = false
	TweenColor(FLY, Color3.fromRGB(255, 150, 150))
	local CHAR = PLR.Character
	local HUM = CHAR:FindFirstChildOfClass("Humanoid")
	if HUM then else return end
	if INVISIBLE_VALUE == false then TweenColor(INVISIBLE, Color3.fromRGB(107, 255, 137))
		CHAR:PivotTo(CHAR.PrimaryPart.CFrame * CFrame.Angles(math.rad(0),math.rad(0),math.rad(0)))
		INVISIBLE_VALUE = true
		--HUM.CameraOffset = Vector3.new(0,-OFFSET,0)
		WALK_PART = Instance.new("Part",workspace)
		WALK_PART.Anchored = true
		WALK_PART.Size = Vector3.new(10000,0.5,10000)
		WALK_PART.CanQuery = false
		WALK_PART.CastShadow = false
		WALK_PART.Transparency = 1 -- what the sigma
		WALK_PART.Position = HUM.RootPart.Position + Vector3.new(0,OFFSET-3.5,0)
		OLD_Y = HUM.RootPart.Position.Y
		CHAR:PivotTo(CFrame.new(HUM.RootPart.CFrame.Position + Vector3.new(0,OFFSET,0)))
		-- CREATION OF FAKE CHAR
		FAKE_CHAR = GET_FAKECHAR(CHAR)
		CAM.CameraSubject = FAKE_CHAR.Humanoid
		--CHAR.PrimaryPart = FAKE_CHAR:FindFirstChild("HumanoidRootPart")
		task.spawn(function()
			repeat task.wait()
				for i,v in pairs(FAKE_CHAR:GetChildren()) do
					if v:IsA("BasePart") then
						local COUNTER_PART = CHAR:FindFirstChild(v.Name)
						local OFFSET = COUNTER_PART.Position + Vector3.new(0,-OFFSET,0)
						v.Position = OFFSET
						v.Orientation = COUNTER_PART.Orientation
					end
				end
			until INVISIBLE_VALUE == false end)
		-- END OF FAKE CHAR
	else TweenColor(INVISIBLE, Color3.fromRGB(255, 150, 150))
		CHAR:PivotTo(CHAR.PrimaryPart.CFrame * CFrame.Angles(math.rad(0),math.rad(0),math.rad(0)))
		INVISIBLE_VALUE = false
		FAKE_CHAR:Destroy()
		WALK_PART:Destroy()
		CHAR:PivotTo(CFrame.new(HUM.RootPart.Position.X, OLD_Y, HUM.RootPart.Position.Z))
		CAM.CameraSubject = HUM
		--CHAR.PrimaryPart = CHAR:FindFirstChild("HumanoidRootPart")
	end
	HUM.RootPart.Anchored = false
end

INVISIBLE.MouseButton1Click:Connect(VISIBILITY)

