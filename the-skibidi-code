--- IGNORE OPEN UI AND UI DRAG
local Debounce = true
local Debounce2 = true
Button.MouseButton1Click:Connect(function()
	if Debounce == false then
		local SIZE, POS = UDim2.new(0,0,0,0), UDim2.new(0.5,0,0.5,0)
		TweenService:Create(Back, TweenInfo.new(0.25,Enum.EasingStyle.Quad),{Position = POS, Size = SIZE}):Play()
		Back.Interactable = false
		Debounce = true
		Button.Text = "OPEN"
	else
		local SIZE, POS = UDim2.new(0.555, 0,0.515, 0), UDim2.new(0.222, 0,0.242, 0)
		TweenService:Create(Back, TweenInfo.new(0.25,Enum.EasingStyle.Quad),{Position = POS, Size = SIZE}):Play()
		Back.Interactable = true
		Debounce = false
		Button.Text = "CLOSE"
	end
end)
local TWEEN_INFO = TweenInfo.new(0.25,Enum.EasingStyle.Quad)
Close.MouseButton1Click:Connect(function()
	if Debounce2 == true then
		local SIZE, POS = UDim2.new(1,0,0.1,0), UDim2.new(0,0,0,0)
		TweenService:Create(Menu,TWEEN_INFO,{Position = POS, Size = SIZE}):Play()
		TweenService:Create(FakeFrame,TWEEN_INFO,{BackgroundTransparency = 0}):Play()
		Menu.Interactable = false
		Debounce2 = false
	else
		local SIZE, POS = UDim2.new(1,0,1,0), UDim2.new(0,0,0,0)
		TweenService:Create(Menu,TWEEN_INFO,{Position = POS, Size = SIZE}):Play()
		TweenService:Create(FakeFrame,TWEEN_INFO,{BackgroundTransparency = 1}):Play()
		Menu.Interactable = true
		Debounce2 = true
	end
end)

local UIS = game:GetService('UserInputService')
local dragToggle = nil
local dragSpeed = 0.25
local dragStart = nil
local startPos = nil

local function updateInput(input)
	local delta = input.Position - dragStart
	local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
		startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	game:GetService('TweenService'):Create(Back, TweenInfo.new(dragSpeed), {Position = position}):Play()
end

Back.InputBegan:Connect(function(input)
	if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
		dragToggle = true
		dragStart = input.Position
		startPos = Back.Position
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragToggle = false
			end
		end)
	end
end)

UIS.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		if dragToggle then
			updateInput(input)
		end
	end
end)
--- IGNORE OPEN UI AND UI DRAG

function TweenColor(TEXTLABEL, COLOR) TweenService:Create(TEXTLABEL, TweenInfo.new(0.25, Enum.EasingStyle.Quad), {TextColor3 = COLOR}):Play() end

-- ESP CODE

local ESP_VALUE = false

local BEAM_COLOR = ColorSequence.new({
	ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 0, 0)),
	ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 0, 0))
})

function CREATE_BEAM(TORSO,E_TORSO)
	local Beam = Instance.new("Beam",workspace)
	Beam.Color = BEAM_COLOR
	Beam.LightEmission = 1
	Beam.LightInfluence = 1
	Beam.Width0 = 0.25
	Beam.Width1 = 0.25
	Beam.FaceCamera = true
	Beam.Name = "ESP_"..PLR.Name
	local Attach1 = Instance.new("Attachment",TORSO) Attach1.Name = "ESP_"..PLR.Name
	local Attach2 = Instance.new("Attachment",E_TORSO) Attach2.Name = "ESP_"..PLR.Name
	Beam.Attachment0 = Attach1
	Beam.Attachment1 = Attach2
	local Highlight = Instance.new("Highlight",E_TORSO.Parent)
	Highlight.Name = "ESP_"..PLR.Name
end

local TargetClasses = {"Attachment","Highlight","Beam"}
function DESTROY_BEAM()
	for i,v in pairs(workspace:GetDescendants()) do
		if table.find(TargetClasses,v.ClassName) and v.Name == "ESP_"..PLR.Name then
			v:Destroy()
		end
	end
end

function GET_HUMANOIDS()
	if ESP_VALUE == false then TweenColor(ESP, Color3.fromRGB(107, 255, 137))
		ESP_VALUE = true
		task.spawn(function() repeat
				for i,v in pairs(workspace:GetDescendants()) do
					local HIGHLIGHT = v.Parent:FindFirstChildOfClass("Highlight")
					local HUM = v.Parent:FindFirstChildOfClass("Humanoid")
					local NAME = v.Name
					if NAME == "Humanoid" and v.Parent.Name ~= PLR.Name and HUM and not HIGHLIGHT then
						local TORSO,E_TORSO = PLR.Character:FindFirstChild("Torso"),v.Parent.PrimaryPart
						CREATE_BEAM(TORSO,E_TORSO)
					end
				end
				task.wait(1)
			until ESP_VALUE == false end)
	else TweenColor(ESP, Color3.fromRGB(255, 150, 150))
		ESP_VALUE = false
		DESTROY_BEAM()
	end
end

ESP.MouseButton1Click:Connect(GET_HUMANOIDS)

